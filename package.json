{
  "name": "obsidian-2hoplink",
  "version": "1.0.1",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "tsc": "tsc -noEmit",
    "tsc:watch": "tsc -noEmit --watch",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json package.json && npm run check",
    "lint": "biome lint --write",
    "format": "biome format --write && remark . --output",
    "check": "biome check --write  --diagnostic-level=warn",
    "ci": "biome ci --diagnostic-level=warn",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.19.0",
    "esbuild-plugin-copy": "^2.0.1",
    "husky": "^9.1.7",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "remarkConfig": {
    "settings": {
      "bullet": "-"
    },
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "dependencies": {
    "esbuild-sass-plugin": "^2.4.5",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
